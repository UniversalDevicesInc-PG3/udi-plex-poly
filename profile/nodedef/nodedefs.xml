<nodeDefs>
    <!-- Plex Webhook NodeServer for PolyglotV2 -->
    <!-- Plex Controller -->
    <nodeDef id="plexcontroller" nls="ctl">
        <sts>
			<st id="ST" editor="bool" />
            <st id="GV0" editor="port" />
			<st id="GV1" editor="count" />
            <st id="GV2" editor="trigger" />
		</sts>
        <cmds />
    </nodeDef>
    <!-- node/client -->
    <nodeDef id="plexclient" nls="plexcl">
        <sts>
			<st id="ST" editor="bool" />
			<st id="GV0" editor="count" />
            <st id="GV1" editor="plexevent" />
            <st id="GV2" editor="plexlib" />
            <st id="GV3" editor="plexmedia" />
            <st id="GV4" editor="plexrate" />
            <st id="GV5" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="RESET" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
